@model IEnumerable<pawprint.Models.tbl_poblacion>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_especies.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbl_especies.Nombre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdPob }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IdPob }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdPob })
        </td>
    </tr>
}

</table>
