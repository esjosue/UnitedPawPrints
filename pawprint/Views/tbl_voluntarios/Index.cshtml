@model IEnumerable<pawprint.Models.tbl_voluntarios>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Edad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_usuarios.Nombre)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Edad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_usuarios.Nombre)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.IdVoluntario }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.IdVoluntario }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.IdVoluntario })
            </td>
        </tr>
    }

</table>
</body>
</html>
